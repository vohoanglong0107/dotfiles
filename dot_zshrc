# vim:ft=zsh

# Must be sourced before zsh-syntax-highlighting is loaded.
source ~/.zsh/external/catppuccin-zsh-syntax-highlighting/themes/catppuccin_mocha-zsh-syntax-highlighting.zsh
source ~/.zsh/external/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

source ~/.zsh/external/zsh-autosuggestions/zsh-autosuggestions.zsh

source ~/.zsh/external/wezterm.sh

source ~/.zsh/completions.zsh

source ~/.zsh/tools.zsh

source ~/.zsh/envs.zsh

source ~/.zsh/aliases.zsh

eval "$(starship init zsh)"
